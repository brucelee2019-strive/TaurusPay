<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/cz_kchf_bg">

        <TextView
            android:id="@+id/tv_acount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="admin"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView74"
            app:layout_constraintVertical_chainStyle="spread"/>

        <TextView
            android:id="@+id/textView73"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="卡池总额:"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/tv_acount"
            app:layout_constraintStart_toEndOf="@+id/textView74"
            app:layout_constraintTop_toTopOf="@+id/tv_acount" />

        <TextView
            android:id="@+id/tv_live"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="代理"
            android:textColor="@color/color_match_type_3"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/tv_acount"
            app:layout_constraintStart_toEndOf="@+id/tv_acount"
            app:layout_constraintTop_toTopOf="@+id/tv_acount"
            android:visibility="invisible"/>

        <TextView
            android:id="@+id/tv_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="0.00"
            android:textColor="@color/f54343"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView73"
            app:layout_constraintStart_toEndOf="@+id/textView73"
            app:layout_constraintTop_toTopOf="@+id/textView73"/>

        <TextView
            android:id="@+id/textView67"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="下载链接:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="@+id/tv_acount"
            app:layout_constraintTop_toBottomOf="@+id/textView75"
            app:layout_constraintBottom_toTopOf="@+id/textView68"/>

        <TextView
            android:id="@+id/textView68"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="推广码:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView67"
            app:layout_constraintTop_toBottomOf="@+id/textView67" />

        <TextView
            android:id="@+id/tv_down_link"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="http://115.144.238.240:8090/index.html"
            android:textSize="10sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/textView67"
            app:layout_constraintStart_toEndOf="@+id/textView67"
            app:layout_constraintTop_toTopOf="@+id/textView67" />

        <Button
            android:id="@+id/btn_copy_link"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/touch_bg"
            android:gravity="center"
            android:text="复制"
            android:textColor="@color/color_black_0"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_down_link"
            app:layout_constraintStart_toEndOf="@+id/tv_down_link"
            app:layout_constraintTop_toTopOf="@+id/tv_down_link" />

        <TextView
            android:id="@+id/tv_broadcast_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="007"
            android:textSize="14sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/textView68"
            app:layout_constraintStart_toEndOf="@+id/textView68"
            app:layout_constraintTop_toTopOf="@+id/textView68" />

        <Button
            android:id="@+id/btn_copy_code"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/touch_bg"
            android:gravity="center"
            android:text="复制"
            android:textColor="@color/color_black_0"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_broadcast_code"
            app:layout_constraintStart_toEndOf="@+id/tv_broadcast_code"
            app:layout_constraintTop_toTopOf="@+id/tv_broadcast_code" />

        <TextView
            android:id="@+id/textView74"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日下级代售总额:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/textView75"
            app:layout_constraintStart_toStartOf="@+id/tv_acount"
            app:layout_constraintTop_toBottomOf="@+id/tv_acount" />

        <TextView
            android:id="@+id/textView75"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="今日下级代售总额:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/textView67"
            app:layout_constraintStart_toStartOf="@+id/textView74"
            app:layout_constraintTop_toBottomOf="@+id/textView74" />

        <TextView
            android:id="@+id/tv_y_day_despoint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="0.00"
            android:textStyle="bold"
            android:textColor="@color/f54343"
            app:layout_constraintBottom_toBottomOf="@+id/textView74"
            app:layout_constraintStart_toEndOf="@+id/textView74"
            app:layout_constraintTop_toTopOf="@+id/textView74" />

        <TextView
            android:id="@+id/tv_day_despoint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="0.00"
            android:textStyle="bold"
            android:textColor="@color/f54343"
            app:layout_constraintBottom_toBottomOf="@+id/textView75"
            app:layout_constraintStart_toEndOf="@+id/textView75"
            app:layout_constraintTop_toTopOf="@+id/textView75" />

        <TextView
            android:id="@+id/textView78"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日总收益:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/textView74"
            app:layout_constraintStart_toEndOf="@+id/textView73"
            app:layout_constraintTop_toTopOf="@+id/textView74" />

        <TextView
            android:id="@+id/textView79"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="今日总收益:"
            android:textSize="12sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/textView75"
            app:layout_constraintStart_toStartOf="@+id/textView78"
            app:layout_constraintTop_toTopOf="@+id/textView75" />

        <TextView
            android:id="@+id/tv_y_day_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="0.00"
            android:textStyle="bold"
            android:textColor="@color/f54343"
            app:layout_constraintBottom_toBottomOf="@+id/textView78"
            app:layout_constraintStart_toEndOf="@+id/textView78"
            app:layout_constraintTop_toTopOf="@+id/textView78" />

        <TextView
            android:id="@+id/tv_day_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:text="0.00"
            android:textStyle="bold"
            android:textColor="@color/f54343"
            app:layout_constraintBottom_toBottomOf="@+id/textView79"
            app:layout_constraintStart_toEndOf="@+id/textView79"
            app:layout_constraintTop_toTopOf="@+id/textView79" />

        <Button
            android:id="@+id/btn_agency_rule"
            android:layout_width="80dp"
            android:layout_height="25dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/touch_bg"
            android:gravity="center"
            android:text="返点规则"
            android:textColor="@color/color_black_0"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_rate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_rate" />

        <Button
            android:id="@+id/btn_transfer_btn"
            android:layout_width="80dp"
            android:layout_height="25dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/touch_bg"
            android:gravity="center"
            android:text="转账"
            android:textColor="@color/color_black_0"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_broadcast_code"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_broadcast_code" />

        <ImageView
            android:id="@+id/imageView37"
            android:layout_width="25dp"
            android:layout_height="25dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn_transfer_btn"
            app:layout_constraintEnd_toStartOf="@+id/btn_transfer_btn"
            app:layout_constraintTop_toTopOf="@+id/btn_transfer_btn"
            app:srcCompat="@drawable/zhuanzhang" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_agency_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
    </androidx.recyclerview.widget.RecyclerView>

</LinearLayout>